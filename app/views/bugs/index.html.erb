<table class="table">
  <thead>
    <tr>
        <th scope="col">Title</th>
        <th scope="col">Description</th>
        <th scope="col">Priority</th>
        <th scope="col">Status</th>
        <th scope="col">Estimated_time</th>
        <th scope="col">Created By</th>
        <th scope="col">Assigned To</th>
    </tr>
  </thead>

  
  <tbody>
   <% @bugs.each do |bug| %>
        <tr>
            <td><%= bug.title %></th>
            <td ><%= bug.description %></td>
            <td><%= bug.priority %></td>
            <td><%= bug.status %></td>
            <td><%= bug.estimated_time %> Days</td>
            <td><%= (User.find(bug.user_id))[:name] %></td>
            <td><%= (User.find(bug.qa_id))[:name]%></td>
            <td><%= link_to "Edit", edit_bug_path(bug) %></td>
        </tr>
    <% end %>
    
  </tbody>
</table>